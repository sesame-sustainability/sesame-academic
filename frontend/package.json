  ],
  "license": "MIT",
  "scripts": {
    "build": "RESOLVED_SOURCE_VERSION=`git rev-parse HEAD` gatsby build --log-pages",
    "develop": "GATSBY_EXPERIMENTAL_LAZY_DEVJS=true NODE_OPTIONS=\"--max-old-space-size=8192\" gatsby develop",
    "dev": "npm run develop",
    "start": "npm run develop",
    "mobiledev": "npm run develop --host 0.0.0.0",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "invalidate:cdn:prod": "NODE_ENV=production RESOLVED_SOURCE_VERSION=`git rev-parse HEAD` node invalidateCDNCache",
    "invalidate:cdn:staging": "NODE_ENV=staging RESOLVED_SOURCE_VERSION=`git rev-parse HEAD` node invalidateCDNCache",
    "deployS3": "npx -n \"-r dotenv/config\" gatsby-plugin-s3 deploy --yes",
    "deploy:prod-and-invalidate": "NODE_OPTIONS=\"--max-old-space-size=8192\" npm run build && DOTENV_CONFIG_PATH=.env.production npm run deployS3 && npm run invalidate:cdn:prod",
    "deploy:prod": "npm run deploy:prod-and-invalidate && npm run deploy:sentry-release",
    "deploy:staging": "NODE_OPTIONS=\"--max-old-space-size=8192\" GATSBY_ACTIVE_ENV=staging npm run build && DOTENV_CONFIG_PATH=.env.staging npm run deployS3 && npm run invalidate:cdn:staging",
    "deploy:sentry-release": "NODE_ENV=production node createSentryRelease",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "cy:serve": "GATSBY_ACTIVE_ENV=production npm run build && npm run serve",
    "test:e2e:local": "CYPRESS_BASE_URL=http://localhost:8000/ start-server-and-test develop 8000 cy:open",
    "test:e2e:ci": "start-server-and-test cy:serve http://localhost:9000 cy:run",
    "test": "npm run test:e2e:ci",
    "fetch:metadata-mock": "node ./fetchMetadataMock.js && prettier --write \"./__tests__/config/metadataMock.ts\"",
    "update:metadata-types": "tsc --declaration ./__tests__/config/metadataMock.ts && rm ./__tests__/config/metadataMock.js",
    "update:csvs": "node ./data-scripts/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.mit.edu/sesame/sesame-web"
  },
  "bugs": {
    "url": "https://github.mit.edu/sesame/sesame-web/issues"
  }
}
